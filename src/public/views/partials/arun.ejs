<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
	<div class="row placeholders" id="top-artists">
		<h3 class="text-justify">My Most Listened To Artists</h3>
		<%if (topArtists.length < 10) {%>
			<% for (var i=0; i < topArtists.length; i++) {%>
				<div class="col-xs-6 col-sm-2 placeholder">
					<img src="<%= topArtists[i].images[1].url %>" width="200" height="200" class="img-responsive" alt="Generic placeholder thumbnail">
					<p><%= topArtists[i].name %></p>
				</div>
			<%}
      } else {%>
				<% for (var i=0; i < 10; i++) {%>
					<div class="col-xs-6 col-sm-2 placeholder">
						<img src="<%= topArtists[i].images[1].url %>" width="200" height="300" class="img-responsive" alt="Generic placeholder thumbnail">
						<p><%= topArtists[i].name %></p>
					</div>
				<%}}%>

			</div>
		</div>
